{
  "name": "tap-min",
  "version": "1.1.0",
  "description": "Minimal TAP output formatter.",
  "bin": {
    "tap-min": "bin/tap-min"
  },
  "scripts": {
    "test": "npm run test:ok && npm run test:not-ok",
    "test:ok": "cat test/ok.txt | ./bin/tap-min > /dev/null; exit $?",
    "test:not-ok": "cat test/not-ok.txt | ./bin/tap-min > /dev/null; if [[ $? == 0 ]]; then exit 1; fi; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gummesson/tap-min.git"
  },
  "keywords": [
    "test",
    "tap",
    "tape",
    "mocha",
    "reporter"
  ],
  "author": "Ellen Gummesson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gummesson/tap-min/issues"
  },
  "homepage": "https://github.com/gummesson/tap-min",
  "devDependencies": {
    "tape": "^3.5.0"
  },
  "dependencies": {
    "tap-parser": "^0.7.0",
    "through2": "^0.6.3",
    "duplexer": "^0.1.1",
    "hirestime": "^1.0.1",
    "pretty-ms": "^1.0.0",
    "chalk": "^0.5.1"
  }
}
